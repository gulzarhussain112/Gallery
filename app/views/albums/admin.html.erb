<h1>hello admin</h1>

<table class="table table-striped table-light">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">E-mail id</th>
       <th scope="col">Created On</th>
      <th scope="col">Action</th>
    
    </tr>
  </thead>

  <tbody>
      <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
        <td><%= user.created_at.to_s.split(' ')[0]  %></td>
      <td><%= link_to "Delete User", admin_destroy_user_path(user), method: :delete, data: { confirm: "You sure?" } ,class:"btn btn-primary"%></td>

    </tr>
  <% end %>
  </tbody>
  
</table>
